{
  "name": "ProjectInvoice",
  "version": "1.4.35",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "version": "next-version package.json src/components/Footer.vue",
    "version:cache": "next-version src/invoice.appcache",
    "version:minor": "npm run version -- --minor",
    "version:major": "npm run version -- --major",
    "markdown": "node task/parseMarkdown --source=README.md --target=src/views/About.html",
    "icomoon": "node task/icomoon --source icomoon.zip --target public/fonts/ --targetFile src/style/_icons.scss",
    "faker": "node task/fakeData"
  },
  "dependencies": {
    "marked": "^0.3.12",
    "moment": "^2.20.1",
    "register-service-worker": "^1.1.0",
    "sass.js": "^0.10.9",
    "signals": "^1.0.0",
    "vue": "^2.5.13",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-alpha.8",
    "@vue/cli-plugin-eslint": "^3.0.0-alpha.8",
    "@vue/cli-plugin-pwa": "^3.0.0-alpha.8",
    "@vue/cli-service": "^3.0.0-alpha.8",
    "adm-zip": "^0.4.7",
    "commander": "^2.14.1",
    "cypress": "^1.4.2",
    "eslint-plugin-html": "^4.0.2",
    "faker": "^4.1.0",
    "jsdom": "^11.6.2",
    "lint-staged": "^6.0.0",
    "next-version": "^0.11.7",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "vue-template-compiler": "^2.5.13",
    "xml2js": "^0.4.19"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "XgitHooks": {
    "apre-commit": "lint-staged"
  },
  "Xlint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}

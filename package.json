{
  "name": "project-invoice",
  "version": "1.2.3",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "opn \"http://localhost:4201\" & ng serve --port 4201",
    "build": "ng build --prod --env=prod  --aot=false",
    "build:test": "npm run build & npm run build:sw & npm run build:serve",
    "build:serve": "cd dist && live-server --port=4203 --host=localhost --entry-file=/index.html",
    "build:sw": "sw-precache --root=dist/ --config=sw-precache-config.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "version": "next-version package.json .angular-cli.json src/app/component/footer/footer.component.html",
    "version:minor": "npm run version -- --minor",
    "version:major": "npm run version -- --major",
    "markdown": "showdown makehtml -i README.md -o temp/bar.html",
    "sw": "sw-precache --root=dist/ --config=sw-precache-config.js"
  },
  "precommit": {
    "run": "version"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.2.2",
    "@angular/common": "^4.2.2",
    "@angular/compiler": "^4.2.2",
    "@angular/core": "^4.2.2",
    "@angular/forms": "^4.2.2",
    "@angular/http": "^4.2.2",
    "@angular/platform-browser": "^4.2.2",
    "@angular/platform-browser-dynamic": "^4.2.2",
    "@angular/router": "^4.2.2",
    "@ngtools/json-schema": "^1.1.0",
    "bootstrap": "^4.0.0-alpha.6",
    "core-js": "^2.4.1",
    "html2canvas": "^0.5.0-beta4",
    "lodash": "^4.17.4",
    "marked": "^0.3.6",
    "rxjs": "^5.1.0",
    "sass.js": "^0.10.4",
    "signals": "^1.0.0",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "^1.1.1",
    "@angular/compiler-cli": "^4.2.2",
    "@angular/language-service": "^4.2.2",
    "@types/jasmine": "2.5.45",
    "@types/marked": "0.0.28",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.0.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "next-version": "^0.11.7",
    "node-sass": "^4.5.3",
    "opn-cli": "^3.1.0",
    "pre-commit": "^1.2.2",
    "protractor": "~5.1.2",
    "showdown": "^1.7.1",
    "sw-precache": "^5.1.1",
    "ts-node": "~3.0.4",
    "tslint": "~5.3.2",
    "webpack": "^2.6.1",
    "typescript": "^2.3.4"
  }

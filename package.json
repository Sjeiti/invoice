{
  "name": "ProjectInvoice",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "moment": "^2.20.1",
    "register-service-worker": "^1.1.0",
    "signals": "^1.0.0",
    "vue": "^2.5.13",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-alpha.8",
    "@vue/cli-plugin-eslint": "^3.0.0-alpha.8",
    "@vue/cli-plugin-pwa": "^3.0.0-alpha.8",
    "@vue/cli-service": "^3.0.0-alpha.8",
    "cypress": "^1.4.2",
    "eslint-plugin-html": "^4.0.2",
    "lint-staged": "^6.0.0",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "vue-template-compiler": "^2.5.13"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "XgitHooks": {
    "apre-commit": "lint-staged"
  },
  "Xlint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
